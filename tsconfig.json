{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "jsx": "react-native",
    "outDir": "compiled",
    "rootDir": "src",
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "exclude": [
    "android",
    "ios",
    "compiled",
    "node_modules"
   ],
   "types": [
     "react",
     "react-native",
     "jest"
   ],
}
